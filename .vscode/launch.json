    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Django",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/api/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "autoStartBrowser": false,
            //"envFile": "${workspaceFolder}/.env.dev"
        },
        {
            "type": "node-terminal",
            "name": "JS Debugger: Vue.JS",
            "request": "launch",
            "command": "npm run dev",
            "cwd": "${workspaceFolder}/ui",
            //"env": {
            //    "NODE_ENV": "development",
            //    "USERNAME": "testuser",
            //    "PASSWORD": "testpassword",
            //    "WSLENV": "NODE_ENV/p:USERNAME/p:PASSWORD/p"
            //},
            //"envFile": "${workspaceFolder}/.env.dev"
        }
    ]
}