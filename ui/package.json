    "@vitejs/plugin-vue": "^5.1.4",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^8.57.1",
    "eslint-plugin-playwright": "^1.6.2",
    "eslint-plugin-vue": "^9.28.0",
    "jsdom": "^24.1.3",
    "prettier": "^3.3.3",
    "sass-embedded": "^1.79.4",
    "vite": "^5.4.8",
