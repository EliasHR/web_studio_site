    "format": "prettier --write src/",
    "generate-sitemap": "node generate-sitemap.js",
    "ssr:start": "cross-env NODE_ENV=production node server"
  },
  "dependencies": {
    "@unhead/vue": "^1.11.6",
    "@vuelidate/core": "^2.0.3",
    "@vuelidate/validators": "^2.0.4",
