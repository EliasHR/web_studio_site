    "express": "^4.21.0",
    "fs": "^0.0.1-security",
    "maska": "^3.0.2",
    "path": "^0.12.7",
    "pinia": "^2.2.2",
    "swiper": "^11.1.14",
    "vue": "^3.5.10",
    "vue-router": "^4.4.5",
    "cross-env": "^7.0.3"
  },
  "devDependencies": {
    "@unhead/vue": "^1.11.6",
    "imagemin-mozjpeg": "^10.0.0",
    "imagemin-pngquant": "^10.0.0",
